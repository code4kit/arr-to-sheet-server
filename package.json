    "document": "documentation build -f md ./src/ | add-text-to-markdown ./DOCUMENT.md --section 'DocumentationJS' --write",
    "readme": "npx readme-md-generator",
    "test": "mocha"
  },
  "author": "code4kit",
  "contributors": [
    "waricoma"
  ],
  "license": "MIT",
  "homepage": "https://github.com/code4kit/arr-to-sheet-server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code4kit/arr-to-sheet-server.git"
  },
  "bugs": {
    "url": "https://github.com/code4kit/arr-to-sheet-server/issues"
  },
  "engines": {
    "yarn": ">=1.19.1",
    "node": ">=12.11.1"
  },
  "dependencies": {
    "array-to-google-sheets": "^2.0.0",
    "dotenv": "^8.0.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "add-text-to-markdown": "2.0.0",
    "commitizen": "4.0.3",
    "cz-emoji": "1.3.1",
    "documentation": "12.1.4",
    "mocha": "7.0.1",
    "readme-md-generator": "1.0.0",
    "semistandard": "14.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    }
  },
  "semistandard": {
    "global": [
      "describe",
      "it"
    ]
  }

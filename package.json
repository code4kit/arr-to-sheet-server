{
  "name": "arr-to-sheet-server",
  "version": "1.0.0",
  "description": "arr-to-sheet-server ( https://github.com/code4kit/arr-to-sheet-client#readme )",
  "main": "./src/index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "commit": "git cz",
